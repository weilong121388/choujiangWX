page {
  width: 100%;
  height: 100vh;
  background-color: #f7f7f7;
}
.aaa{
  background-color:rgba(255, 255, 255, 1);
  color: #000; 
}
.img {
  width: 100%;
  height: auto;
  background: rgba(255, 255, 255, 1);
  margin-top: 33rpx;
  padding-bottom: 30rpx;
}

.fl {
  width: 92%;
  margin-bottom: 30rpx;
  display: flex;
  border-bottom: 1px #f7f7f7 solid;
  margin: 0 auto;
  padding: 30rpx 0;
  position: relative;
}

.tp {
  width: 202rpx;
  height: 202rpx;
  border-radius: 8rpx;
  flex-shrink: 0;
}

.name {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  align-items: flex-start;
}

.na {
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.jf {
  display: flex;
  align-items: center;
  justify-content: flex-start;
  position: absolute;
  bottom: 30rpx;
}

.jf :nth-child(1) {
  font-size: 40rpx;
  font-weight: 400;
  color: rgba(255, 35, 61, 1);
}

.jf :nth-child(2) {
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(255, 35, 61, 1);
  margin-left: 15rpx;
  margin-top: 10rpx;
}

.fot {
  width: 90%;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin: 0 auto;
  margin-top: 32rpx;
}
